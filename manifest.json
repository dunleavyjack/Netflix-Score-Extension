{
"manifest_version": 2,
"name": "Movie Scores",
"version": "1.0",
"description": "",
"web_accessible_resources" : [
    "images/icon.png",
    "images/imdb_logo_color.png",
    "images/rt_logo_bw.png",
    "images/imdb_logo_bw.png",
    "images/rt_logo_color.png",
    "css/style.css"
],
"background": {
    "scripts": ["background.js"]
},
"browser_action" : { 
    "default_icon" : "images/icon.png"
},
"permissions": [
    "https://www.omdbapi.com/*"
],
"icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
},
"content_scripts" : [
{ 
"matches" : [ 
    "https://www.netflix.com/browse/genre/*"
], 
"js" : [
    "jquery-3.5.1.min.js",
    "content.js"
],
"css" : [
    "css/style.css"
]
}
]
}